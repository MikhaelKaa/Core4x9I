{
    "configurations": [
        {
            "name": "arm_c_cpp_properties",
            "compilerPath": "C:/arm_tool/gcc/bin/arm-none-eabi-gcc.exe",
            "includePath": [
                "C:/arm_tool/gcc/arm-none-eabi/include",
                "C:/arm_tool/gcc/arm-none-eabi/include/sys/",
                "C:/arm_tool/gcc/lib/**",
                "${workspaceRoot}\\Drivers\\**",
                "${workspaceRoot}\\Core\\Src\\**",
                "${workspaceRoot}\\Core\\Inc\\**",
                "${workspaceFolder}/Drivers/STM32F4xx_HAL_Driver/Inc",
                "${workspaceFolder}/Core/Inc",
                "${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32F4xx/Include",
                "${workspaceFolder}/Drivers/CMSIS/Include",
                "${workspaceFolder}/app/retarget",
                "${workspaceFolder}/app/u_read_line",
                "${workspaceFolder}/app/mem",
                "${workspaceFolder}/app/micros",
                "${workspaceFolder}/app/coremark"
            ],
            "defines": [
                "USE_HAL_DRIVER",
                "STM32F429xx",
                "M4",
                "GCC_ARMCM4",
                "ITERATIONS=4096",
                "CLOCKS_PER_SEC=1000000",
                "COREMARK_TIME=micros",
                "HAL_UART_xCallback"                
            ],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "windows-gcc-arm"
        }
    ],
    "version": 4
}